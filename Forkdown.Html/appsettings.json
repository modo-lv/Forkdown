{
  "SeriLog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [{
      "Name": "Console",
      "args": {
        "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
        "outputTemplate": "[{Timestamp:HH:mm:ss.fff} {Level:u4}] {Message:lj}{Exception}{NewLine}"
      }
    }]
  }
}