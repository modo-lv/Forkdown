
<p {{ attrs $1 }}>
{{- if ($1.Kind != "Normal") -}}
<span src="{{ PathToRoot }}/assets/labels/{{ $1.Kind | string.downcase }}.png" class="fd--reserved-label">{{ case $1.Kind
when "X" 
}}🔎{{
when "Warning"
}}⚠️{{
when "Info"
}}ℹ️{{
end }}</span>
{{- end -}}

{{labels $1 | html.escape }}{{ subs $1 }}
</p>
