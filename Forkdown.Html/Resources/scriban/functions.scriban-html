{{-

include 'functions/attributes'
include 'functions/elements'

-}}


{{- func labels
  if ($0.Labels.Count > 0)
    }}<span class="fd--labels">{{
    for $label in $0.Labels
      }}<span class="fd--label">{{$label}}</span>{{
    end
    }}</span>{{
  end
end -}}


{{- func global_anchor

if $0.ExplicitId != ""
  }}<a id="{{ $0.ExplicitId }}" class="fd--global_anchor"></a>{{
end

end -}}